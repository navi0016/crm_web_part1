<h1>Create new Contact</h1>

<form action='/contacts' method='post'>

  <label id="first_name" for="first_name">
    <span>First Name: </span>
  <input id="first_name" type="text" name="first_name" value="">
  </label>

  <label id="last_name" for="last_name">
    <span>Last Name: </span>
  <input id="last_name" type="text" name="last_name" value="">
  </label>

  <label id="email" for="email">
    <span>Email: </span>
  <input id="email" type="text" name="email" value="">
  </label>

  <label id="notes" for="notes">
    <span>Notes: </span>
    <textarea id="notes" name="notes"></textarea>

  </label>

  <label id="submit" for="submit">
    <span>Submit: </span>
  <input id='submit' type='submit' value="New Contact">
  </label>

</form>
