<h1>Contacts</h1>
    <p>There are <span><%=@size %></span> contacts.</p>
<ul>
  <% @contacts.each do |contact| %>
    <li>
      Status: <%if contact.status%>Active<%else%>Inactive<%end%><br>
      Full Name: <a href="/contacts/<%=contact.id%>"><%= contact.full_name %></a>
      <!-- Email: <%= contact.email %> --,>
      <!-- Notes: <%= contact.note %> -->
      <br>Created At: <%=contact.created_at%>
      <br><a href="/contact/<%=contact.id%>/edit">Edit</a> | <a href="/contacts/<%=contact.id%>">Delete</a>
    </li>
  <% end %>
</ul>
